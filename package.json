{
  "name": "electrotransfer",
  "version": "2.1.0",
  "description": "ElectroTransfer - Generador de Layouts de Transferencias - Aplicaci√≥n de escritorio con React + TypeScript + Electron",
  "main": "dist-electron/main.js",
  "homepage": "https://github.com/GanzytoX/electro-transfer",
  "repository": {
    "type": "git",
    "url": "https://github.com/GanzytoX/electro-transfer.git"
  },
  "bugs": {
    "url": "https://github.com/GanzytoX/electro-transfer/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "vite",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist dist-electron dist-react",
    "lint": "tsc --noEmit",
    "build:all": "npm run clean && npm run build",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "keywords": [
    "electrotransfer",
    "transferencias",
    "layouts",
    "spei",
    "electron",
    "react",
    "typescript",
    "banking",
    "fintech"
  ],
  "author": {
    "name": "GanzytoX",
    "email": "gonzalo.gb11.ggb@gmail.com"
  },
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@ant-design/icons": "^6.1.0",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/xlsx": "^0.0.35",
    "@vitejs/plugin-react": "^5.1.3",
    "antd": "^6.2.3",
    "electron": "^40.2.1",
    "electron-builder": "^24.13.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "xlsx": "^0.18.5"
  },
  "dependencies": {
    "@electron/remote": "^2.1.3"
  },
  "build": {
    "appId": "com.ganzytox.electrotransfer",
    "productName": "ElectroTransfer",
    "directories": {
      "output": "dist",
      "buildResources": "assets"
    },
    "files": [
      "dist-react/**/*",
      "dist-electron/**/*",
      "assets/**/*",
      "package.json",
      "!**/*.py",
      "!src/**/*",
      "!electron/**/*"
    ],
    "extraFiles": [],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png",
      "category": "Office"
    }
  }
}
